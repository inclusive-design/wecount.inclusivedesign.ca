<nav class="primary-nav" aria-label="Primary Navigation">
  {% set navItems = collections.pages | eleventyNavigation %}
  {%- for item in navItems %}
    {%- if item.order > 0 %}
    <a {% if page.url === item.url %}aria-current="page" {% endif %}href="{{ item.url }}">{{ item.key }}</a>
    {%- endif %}
  {%- endfor %}
  <a {% if page.url === '/news/' %}aria-current="page" {% endif %}href="/news/">News</a>
  <a {% if page.url === '/views/' %}aria-current="page" {% endif %} href="/views/">Views</a>
</nav>
