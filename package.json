{
	"name": "we_count",
	"version": "1.0.0",
	"description": "We Count Project",
	"author": "Ted Thompson",
	"private": true,
	"scripts": {
		"dev": "nuxt",
		"build": "nuxt build",
		"start": "nuxt start",
		"generate": "nuxt generate --fail-on-error",
		"lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
		"lint:css": "stylelint \"**/**.{css,vue}\"",
		"lint:md": "markdownlint \"**/***.md\" --ignore-path .gitignore --ignore LICENSE.md",
		"lint-fix:js": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
		"lint-fix:css": "stylelint \"**/**.{css,vue}\" --fix",
		"lint-fix:md": "markdownlint \"**/***.md\" --ignore-path .gitignore --ignore LICENSE.md --fix",
		"lint": "npm-run-all lint:js lint:css lint:md",
		"lint:fix": "npm-run-all lint-fix:*",
		"test": "npm run lint"
	},
	"dependencies": {
		"@nuxtjs/axios": "^5.3.6",
		"@nuxtjs/bulma": "^1.2.6",
		"@nuxtjs/dotenv": "^1.4.0",
		"@nuxtjs/pwa": "^3.0.0-0",
		"bootstrap": "^4.1.3",
		"bootstrap-vue": "^2.0.0",
		"nuxt": "^2.0.0"
	},
	"devDependencies": {
		"@nuxtjs/eslint-config": "^1.0.1",
		"@nuxtjs/eslint-module": "^1.0.0",
		"babel-eslint": "^10.0.1",
		"eslint": "^6.1.0",
		"eslint-config-fluid": "^1.4.0",
		"eslint-plugin-nuxt": ">=0.4.2",
		"markdownlint-cli": "^0.22.0",
		"npm-run-all": "^4.1.5",
		"stylelint": "^13.1.0",
		"stylelint-config-recommended": "^3.0.0",
		"stylelint-order": "^4.0.0"
	}
}
